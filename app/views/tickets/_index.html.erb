<div class="container text-center">
  <div class="row">
    <% tickets.each do |ticket| %>
      <%= link_to ticket_path(ticket) do%>
        <div class="col-sm-4 one-ticket">
          <img src="Evento<%=(1..4).to_a.sample%>.png" class="img-responsive" style="width:100%" alt="Image">
          <strong>
            <p><%= ticket.name %></p>
            <p><%= ticket.date.strftime("%d/%m/%Y") %></p>
            <p>R$ <%= ticket.price + ticket.taxe %></p>
          </strong>
        </div>
      <% end %>
    <% end %>
  </div>
</div><br>